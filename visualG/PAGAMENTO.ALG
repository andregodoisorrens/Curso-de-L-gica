Algoritmo "Pagamento"

Var

   nome : caractere
   Vh, pagamento : real
				 Ht : inteiro
Inicio

      escreva("Nome: ")
      leia(nome)
      escreva("Valor por hora: ")
      leia(Vh)
      escreva("Horas Ttrabalhadas: ")
      leia(ht)

      pagamento <- Vh * Ht

      escreval(pagamento)

Fimalgoritmo